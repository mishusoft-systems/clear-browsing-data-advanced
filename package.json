{
  "name": "browser-cache-cleaner",
  "version": "1.0.0",
  "description": "Browser cache cleaner",
  "homepage": "https://github.com/mralaminahamed/browser-cache-cleaner/",
  "bugs": {
    "url": "https://github.com/mralaminahamed/browser-cache-cleaner/issues",
    "email": "alamin@mishusoft.com"
  },
  "author": {
    "name": "Al Amin Ahamed",
    "email": "alamin@mishusoft.com",
    "url": "https://github.com/mralaminahamed/"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/al-amin-ahamed"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mralaminahamed/browser-cache-cleaner.git"
  },
  "keywords": [
    "cache",
    "browser cache",
    "cache cleaner",
    "browser cache cleaner",
    "browser data resetter"
  ],
  "license": "GPL-3.0-only",
  "scripts": {
    "start": "webpack-dev-server --mode development --env target=chrome",
    "start:ext": "web-ext run",
    "build": "webpack",
    "build:chrome": "webpack --env target=chrome",
    "lint": "web-ext lint",
    "package": "yarn update:version && yarn build && web-ext build --overwrite-dest -a dist-firefox-package",
    "package:chrome": "yarn update:version && yarn build:chrome && web-ext build --overwrite-dest -s dist-chrome -a dist-chrome-package",
    "watch": "webpack -w",
    "watch:chrome": "webpack -w --env target=chrome",
    "update:addon": "node ./src/utils/update-addon",
    "update:version": "node ./src/utils/sync-app-version",
    "upgrade:packages": "npx npm-check-updates -u",
    "zip-src": "node ./src/utils/zip-src"
  },
  "devDependencies": {
    "@types/sass": "^1.43.1",
    "@webpack-cli/info": "^1.4.1",
    "@webpack-cli/serve": "^1.6.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "events": "^3.3.0",
    "file-loader": "^6.2.0",
    "handlebars": "^4.7.7",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^2.6.0",
    "sass-loader": "^12.6.0",
    "semver-parser": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "web-ext-plugin": "^1.6.0",
    "web-ext-types": "^3.2.1",
    "webextension-polyfill": "^0.8.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-preprocessor": "^0.1.12",
    "webpack-preprocessor-loader": "^1.1.4"
  },
  "webExt": {
    "sourceDir": "dist-firefox"
  },
  "dependencies": {
    "@types/webextension-polyfill": "^0.8.2",
    "archiver": "^5.3.0",
    "web-ext": "^6.7.0"
  }
}
